[
	{ "key": "cmd+9",                "command": "editor.action.jumpToBracket",
                                     "when": "editorTextFocus" },
    { "key": "alt+shift+e",          "command": "editor.action.previewDeclaration",
                                     "when": "editorTextFocus" },
    { "key": "alt+e",                "command": "editor.action.goToDeclaration",
                                     "when": "editorTextFocus" },
    { "key": "alt+r",                "command": "editor.action.goToTypeDeclaration" },
    { "key": "cmd+r",                "command": "workbench.action.gotoSymbol" },
    
    { "key": "shift+cmd+g",          "command": "workbench.view.git" },
    { "key": "cmd+k cmd+b",          "command": "workbench.action.toggleSidebarVisibility" }
]